# Credenciais do LinkedIn
LINKEDIN_EMAIL=seu-email@exemplo.com
LINKEDIN_PASSWORD=sua-senha

# Chave de API do Google para o Gemini
GOOGLE_API_KEY=sua-chave-api-google

# Configurações do navegador
BROWSER_HEADLESS=True
BROWSER_TIMEOUT=30000

# Configurações de limitação de taxa
REQUESTS_PER_MINUTE=5
JITTER_MIN_MS=500
JITTER_MAX_MS=2000

# Configurações de retry
MAX_RETRIES=3
RETRY_DELAY=5.0
BACKOFF_FACTOR=2.0

# Estratégia de busca para o ProfileFinder
# Opções: "google", "bing", "direct"
SEARCH_STRATEGY=google
